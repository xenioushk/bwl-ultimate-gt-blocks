{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bwl-ultimate-gt-blocks/button",
	"version": "1.0.0",
	"title": "Bwl Custom Button",
	"category": "widgets",
	"icon": "smiley",
	"description": "BWL Custom Button",
	"example": {},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"gradients": true
		}
	},
	"attributes": {
		"text": { "type": "string", "default": "" },
		"size": { "type": "string", "default": "large" }
	},
	"textdomain": "bwl-ultimate-gt-blocks"
}
